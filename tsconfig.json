{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es2021",
		"module": "commonjs",
		"baseUrl": ".",
		"outDir": "./dist",
		"lib": [
			"es2021",
			"dom",
			"ES2021.String"
		],
		"sourceMap": true,
		"declaration": true,
		"strict": true,
		"moduleResolution": "node",
		"typeRoots": [
			"node_module/@types",
			"src/modules/types",
			"types"
		],
		"types": [
		],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
	},
	"exclude": [
		"node_modules",
		"test",
	]
}